include ../page-header/page-header
include ../analityc-widgethead/analityc-widgethead
include ../analityc-graphics/analityc-graphics
include ../analityc-table/analityc-table
include ../../blocks/table/table
include ../../blocks/checkbox/checkbox
include ../../blocks/legend/legend
include ../../blocks/dropdown-block/dropdown-block

mixin depfin_analytics_income()
	.main._basic._analitycs-income
		section.basic-budget
			.wrapper.wrapper_params-income
				+page-header('Доходы бюджета', false, true)

				// Tab 1
				.analityc-widget.analityc-widget_params-income
					+analityc-widgethead-analityc-income()
					.analityc-widget-income.analityc-widget-income_pace(data-filter='pace')
						+analityc-graphics-line-vertical-alt(getData('graphic-lines-analityc-income'))
						+legend-checkbox(getData('legend-analityc-income'))
					.analityc-widget-income.analityc-widget-income_share(data-filter='share')
						+analityc-graphics-bars(getData('legend-analityc-bar1'))
						+legend-checkbox(getData('legend-analityc-income'))
					.analityc-widget-income.analityc-widget-income_done(data-filter='done')
						+analityc-graphics-bars(getData('legend-analityc-bar2'))
						+legend-checkbox(getData('legend-analityc-income'))
					.analityc-widget-income.analityc-widget-income_year
						+analityc-graphics-bars-year(getData('legend-analityc-bar3'))
						+legend-checkbox(getData('legend-analityc-income'))
					.analityc-widget-income.analityc-widget-income_bars-full
						+analityc-graphics-bars-alt(getData('legend-analityc-bar4'))
						+legend-basket(getData('legend-analityc-income-short'))
					.analityc-widget-income.analityc-widget-income_bars
						+analityc-graphics-bars2(getData('legend-analityc-bar5'))
						+legend-basket(getData('legend-analityc-income'))
					
					+analityc-table()._params
						+table-aincome(getData('main'))
					a(href="/").analityc-more Перейти в&nbsp;раздел бюжет москвы
			+dropdown()
